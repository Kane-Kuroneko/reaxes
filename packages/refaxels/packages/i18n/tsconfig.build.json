{
	"compilerOptions" : {
		"allowJs" : true ,
		"allowSyntheticDefaultImports" : true ,
		"allowUnusedLabels" : true ,
		"baseUrl" : "." ,
		"declaration" : true ,
		"declarationMap" : false ,
		"esModuleInterop" : true ,
		"experimentalDecorators" : true ,
		"jsx" : "react" ,
		"resolveJsonModule" : true ,
		"strictFunctionTypes" : false ,
		"allowUmdGlobalAccess" : true ,
		"allowArbitraryExtensions" : true ,
		"allowImportingTsExtensions" : true ,
		"strict" : false ,
		"strictBindCallApply" : false ,
		"strictNullChecks" : false ,
		"allowUnreachableCode" : true ,
		"module" : "ESNext" ,
		"checkJs" : true ,
		"moduleResolution" : "node" ,
		"noImplicitAny" : false ,
		"noImplicitReturns" : false ,
		"noImplicitUseStrict" : false ,
		"outDir" : "dist/types" ,
		"noUnusedLocals" : false ,
		"noUnusedParameters" : false ,
		"preserveConstEnums" : true ,
		"removeComments" : true ,
		"sourceMap" : true ,
		"target" : "ESNext" ,
		"emitDeclarationOnly" : true ,
		"skipLibCheck" : true ,
		"paths" : {
			"reaxes-utils": ["node_modules/reaxes-utils"],
			"reaxes-toolkit": ["../../../reaxes-toolkit"],
			"reaxes": ["node_modules/reaxes"],
			"reaxes/*": ["node_modules/reaxes/*"],
			"reaxes-react": ["node_modules/reaxes-react"],
			"reaxes-vue2": ["node_modules/reaxes-vue2"],
			"reaxes-vue3": ["node_modules/reaxes-vue3"],
			"reaxels/*": ["node_modules/reaxels/*"],
			"refaxels/*": ["node_modules/refaxels/packages/*"]
		}
	} ,
	"exclude" : [ "webpack.partial.ts", "node_modules" , "dist" , ".git" , ".idea" , ".svn" ]
}
