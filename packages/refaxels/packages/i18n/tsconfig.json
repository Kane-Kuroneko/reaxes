{
	"compilerOptions" : {
		"allowJs" : true ,
		"allowSyntheticDefaultImports" : true ,
		"allowUnusedLabels" : true ,
		"baseUrl" : "." ,
		"declaration" : true ,
		"declarationMap" : false ,
		"esModuleInterop" : true ,
		"experimentalDecorators" : true ,
		"jsx" : "react" ,
		"resolveJsonModule" : true ,
		"strictFunctionTypes" : false ,
		"allowUmdGlobalAccess" : true ,
		"allowArbitraryExtensions" : true ,
		"allowImportingTsExtensions" : true ,
		"strict" : false ,
		"strictBindCallApply" : false ,
		"strictNullChecks" : false ,
		"allowUnreachableCode" : true ,
		"module" : "ESNext" ,
		"checkJs" : true ,
		"moduleResolution" : "node" ,
		"noImplicitAny" : false ,
		"noImplicitReturns" : false ,
		"noImplicitUseStrict" : false ,
		"outDir" : "dist/types" ,
		"noUnusedLocals" : false ,
		"noUnusedParameters" : false ,
		"preserveConstEnums" : true ,
		"removeComments" : true ,
		"sourceMap" : true ,
		"target" : "ESNext" ,
		"emitDeclarationOnly" : true ,
		"skipLibCheck" : true ,
		"paths" : {
			"reaxes-utils": ["../../../reaxes-utils"],
			"reaxes-toolkit": ["../../../reaxes-toolkit"],
			"reaxes": ["../../../reaxes"],
			"reaxes/*": ["../../../reaxes/*"],
			"reaxes-react": ["../../../reaxes-react"],
			"reaxes-vue2": ["../../../reaxes-vue2"],
			"reaxes-vue3": ["../../../reaxes-vue3"],
			"reaxels/*": ["../../../packages/reaxels/*"],
			"refaxels/*": ["../../../refaxels/packages/*"]
		}
	} ,
	"exclude" : [ "webpack.partial.ts", "node_modules" , "dist" , ".git" , ".idea" , ".svn" ]
}
